      <view bindtap='toStart' wx:else class="startBut">START</view>
      <view class='changeBut b4 {{loaction!=3&&"noChooseItem"}}' data-index='3'  style="background-color:{{showList[3].color}};" >{{showList[3].name}}</view>
     
      
    </view>
    <view class='h3'>
