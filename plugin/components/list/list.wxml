    <view class='h1'>
      <view class='changeBut b1 {{loaction!="0"&&"noChooseItem"}}' data-index='0'  style="background-color:{{showList[0].color}};" >{{showList[0].name}}</view>
      <view class='changeBut b2 {{loaction!=1&&"noChooseItem"}}' data-index='1'  style="background-color:{{showList[1].color}};" >{{showList[1].name}}</view>
      <view class='changeBut b3 {{loaction!=2&&"noChooseItem"}}' data-index='2'  style="background-color:{{showList[2].color}};" >{{showList[2].name}}</view>
    </view>
    <view class='h2'>
